<Window x:Class="SemPrace.Dialog.DialogConfirm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SemPrace.Dialog"
        mc:Ignorable="d"
        Title="Potvrzeni" Height="200
        " Width="250
        
        
        " ResizeMode="NoResize" WindowStyle="ToolWindow">
    <Grid Height="184" VerticalAlignment="Center" HorizontalAlignment="Center" Width="282">
        <Button x:Name="ConfirmButton" Content="Potvrdit" Margin="140,75,50,74" Width="60" Height="35" BorderThickness="0,0,0,0" Background="#FFC5DEA3" Click="ConfirmButton_Click" />
        <Button x:Name="CancelButton" Content="Zrušit" Margin="50,75,140,74" Width="60" Height="35" BorderThickness="0,0,0,0" Background="#FFF5C3C3" Click="CancelButton_Click"/>

    </Grid>
